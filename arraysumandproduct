import java.util.*;
class arraysumproduct{
    public static void main(String[]args)
    {
        int arr[]={1,3,6,5};
        int sum=0;
        int product=1;
        for (int i=0;i<arr.length;i++)
        {
            sum+=arr[i];
            product*=arr[i];
        }
        System.out.println("array of sum:"+sum);
        System.out.println("array of product:"+product);
    }
}

/*op:
array of sum:15
array of product:90

*/
