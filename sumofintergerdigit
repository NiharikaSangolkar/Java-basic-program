// Write a Java program and compute the sum of an integer's digits.

// Test Data:
// Input an intger: 25
import java.util.*;
class Main{
  public static void main(String[]args)
  {
  Scanner sc=new Scanner(System.in);
  System.out.println("enter digit:");
  int n=sc.nextInt();
  int sum=0;
  while(n!=0)
  {
    sum=sum+(n%10);
    n=n/10;
  }
  System.out.println("sum of digit"+n+"="+sum);
  
  }
}
